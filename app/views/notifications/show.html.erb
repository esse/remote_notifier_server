<p id="notice"><%= notice %></p>

<p>
  <b>Client:</b>
  <%= @notification.client_id %>
</p>

<p>
  <b>Message:</b>
  <%= @notification.message %>
</p>

<p>
  <b>Msg class:</b>
  <%= @notification.msg_class %>
</p>

<p>
  <b>Backtrace:</b>
  <%= @notification.backtrace.gsub("\n", "<br />").html_safe %>
</p>

<%= link_to 'Back', client_notifications_path(@notification.client) %>
